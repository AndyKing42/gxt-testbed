<!DOCTYPE ui:UiBinder SYSTEM 'http://dl.google.com/gwt/DTD/xhtml.ent'>
<ui:UiBinder
  xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:button='urn:import:com.sencha.gxt.widget.core.client.button'
  xmlns:container='urn:import:com.sencha.gxt.widget.core.client.container'
  xmlns:form='urn:import:com.sencha.gxt.widget.core.client.form'
  xmlns:gxt='urn:import:com.sencha.gxt.widget.core.client'>
  <!--====== Padding =========================================================================== -->
  <ui:with
    type='com.sencha.gxt.core.client.util.Padding'
    field='padding_3_3_3_3'>
    <ui:attributes
      top='3'
      right='3'
      bottom='3'
      left='3'/>
  </ui:with>
  <!--====== Margins =========================================================================== -->
  <ui:with
    field='margins_3_3_3_3'
    type='com.sencha.gxt.core.client.util.Margins'>
    <ui:attributes
      bottom='3'
      left='3'
      right='3'
      top='3'/>
  </ui:with>
  <ui:with
    field='margins_0_3_3_0'
    type='com.sencha.gxt.core.client.util.Margins'>
    <ui:attributes
      bottom='0'
      left='3'
      right='3'
      top='0'/>
  </ui:with>
  <ui:with
    field='margins_0_0_0_0'
    type='com.sencha.gxt.core.client.util.Margins'>
    <ui:attributes
      bottom='0'
      left='0'
      right='0'
      top='0'/>
  </ui:with>
  <!--====== BorderLayoutData ================================================================== -->
  <ui:with
    field='northData'
    type='com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData'>
    <ui:attributes
      margins='{margins_3_3_3_3}'
      size='125'
      split='true'/>
  </ui:with>
  <ui:with
    field='westData'
    type='com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData'>
    <ui:attributes
      margins='{margins_0_3_3_0}'
      size='50'
      split='true'/>
  </ui:with>
  <ui:with
    field='eastData'
    type='com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData'>
    <ui:attributes
      margins='{margins_0_3_3_0}'
      size='100'/>
  </ui:with>
  <ui:with
    field='southData'
    type='com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData'>
    <ui:attributes
      margins='{margins_3_3_3_3}'
      size='100'/>
  </ui:with>
  <!--====== BoxLayoutData ===================================================================== -->
  <ui:with
    type="com.sencha.gxt.widget.core.client.container.BoxLayoutContainer.BoxLayoutData"
    field="boxLayoutDataMargins_3_3_3_3">
    <ui:attributes margins="{margins_3_3_3_3}"/>
  </ui:with>
  <!--========================================================================================== -->
  <container:Viewport>
    <container:BorderLayoutContainer borders='true'>
      <container:north layoutData='{northData}'>
        <gxt:ContentPanel headerVisible='false'>
          <container:HBoxLayoutContainer
            hBoxLayoutAlign='TOP'
            pack='START'
            padding='{padding_3_3_3_3}'>
            <container:child layoutData="{boxLayoutDataMargins_3_3_3_3}">
              <button:TextButton
                ui:field='recreateTablesButton'
                text='Recreate Database Tables'
                visible='false'/>
            </container:child>
            <container:child layoutData="{boxLayoutDataMargins_3_3_3_3}">
              <button:TextButton
                ui:field='reloadTestDataButton'
                text='Reload Test Data'
                visible='false'/>
            </container:child>
            <container:child layoutData="{boxLayoutDataMargins_3_3_3_3}">
              <button:TextButton
                ui:field='gaeStuffButton'
                text='GAE Stuff'
                visible='false'/>
            </container:child>
            <container:child layoutData="{boxLayoutDataMargins_3_3_3_3}">
              <container:VBoxLayoutContainer borders='true'>
                <container:child layoutData="{boxLayoutDataMargins_3_3_3_3}">
                  <form:CheckBox
                    ui:field='inlineEditingCheckBox'
                    boxLabel='Inline Editing?'/>
                </container:child>
                <container:child layoutData="{boxLayoutDataMargins_3_3_3_3}">
                  <form:CheckBox
                    ui:field='checkBoxSelectionModelCheckBox'
                    boxLabel='Checkbox Selection Model?'
                    value='true'/>
                </container:child>
                <container:child layoutData="{boxLayoutDataMargins_3_3_3_3}">
                  <form:CheckBox
                    ui:field='rowLevelCommitsCheckBox'
                    boxLabel='Row Level Commits?'
                    value='true'/>
                </container:child>
                <container:child layoutData="{boxLayoutDataMargins_3_3_3_3}">
                  <button:TextButton
                    ui:field='recreateGridButton'
                    text='Recreate Grid'/>
                </container:child>
              </container:VBoxLayoutContainer>
            </container:child>
          </container:HBoxLayoutContainer>
        </gxt:ContentPanel>
      </container:north>
      <container:west layoutData='{westData}'>
        <gxt:ContentPanel headerVisible='false'>
          <gwt:Label text='West'/>
        </gxt:ContentPanel>
      </container:west>
      <container:center>
        <gxt:ContentPanel
          ui:field='centerPanel'
          headerVisible='false'/>
      </container:center>
      <container:east layoutData='{eastData}'>
        <gxt:ContentPanel headerVisible='false'>
          <gwt:Label text='East'/>
        </gxt:ContentPanel>
      </container:east>
      <container:south layoutData='{southData}'>
        <gxt:ContentPanel headerVisible='false'>
          <gwt:Label text='South'/>
        </gxt:ContentPanel>
      </container:south>
    </container:BorderLayoutContainer>
  </container:Viewport>
</ui:UiBinder>